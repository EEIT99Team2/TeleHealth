<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="register.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/Members/RegisterSuccess.jsp"></property>
	</bean>

	<bean id="register.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/Members/register.jsp"></property>
	</bean>
	
	<bean id="login.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/index.jsp"></property>
	</bean>
	
	<bean id="login.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/Members/Members02.jsp"></property>
	</bean>
	
	<bean id="forgetPwd.success" class="org.springframework.web.servlet.view.RedirectView">
		<property name="contextRelative" value="true"></property>
		<property name="url" value="/ForgetSuccess.jsp"></property>
	</bean>
	
	<bean id="forgetPwd.error" class="org.springframework.web.servlet.view.InternalResourceView">		
		<property name="url" value="/ForgetPwd.jsp"></property>
	</bean>	
	
	<bean id="selectdata.table" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/page/selectdataTable.jsp"></property>
	</bean>
	
	<bean id="selectRecords.table" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/page/recordsTable.jsp"></property>
	</bean>
	
	<bean id="form.error" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/healthcolumn/ck.jsp"></property>
	</bean>
	
	<bean id="form.ok" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/healthcolumn/ck.jsp"></property>
	</bean>

	<bean id="deletecontentok" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/healthcolumn/Dpublish.jsp"></property>
	</bean>
	<bean id="deletecontenterror" class="org.springframework.web.servlet.view.InternalResourceView">
		<property name="url" value="/healthcolumn/Dpublish.jsp"></property>
	</bean>
	
</beans>
